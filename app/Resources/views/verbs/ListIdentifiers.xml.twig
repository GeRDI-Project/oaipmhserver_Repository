{% extends 'base.xml.twig' %}
{% block payload %}
  <ListIdentifiers>
{% for item in items %}
    <header>
      <identifier>oai:{{ baseUrl }}:{{ item.id }}</identifier> 
      <datestamp>{{ item.timestamp | date('Y-m-d\\Th:m:s\\Z', 'utc')  }}</datestamp>
    </header> 
{% endfor %}
{% if resumptionToken matches '/^resToken.*/' %}
 <resumptionToken expirationDate="2018-04-08T19:14:24Z" cursor="0"> {{ resumptionToken }} </resumptionToken>
{% endif %}
  </ListIdentifiers>
{% endblock %}
