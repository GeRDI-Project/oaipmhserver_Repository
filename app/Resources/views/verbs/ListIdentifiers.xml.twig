{% extends 'base.xml.twig' %}
{% block payload %}
  <ListIdentifiers>
{% for item in items %}
    <header>
      <identifier>oai:{{ baseUrl }}:{{ item.id }}</identifier> 
      <datestamp>{{ item.timestamp | date('Y-m-d\\Th:m:s\\Z', 'utc')  }}</datestamp>
    </header> 
{% endfor %}
{% if resumptionToken is defined %}
 <resumptionToken completeListSize="{{completeListSize}}" cursor="{{cursor}}">{{resumptionToken}}</resumptionToken>
{% endif %}
  </ListIdentifiers>
{% endblock %}
